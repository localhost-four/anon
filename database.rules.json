{
  "rules": {
    ".read": true,
    ".write": true,
    "messages": {
      "$messageId": {
        ".write": "$messageId === auth.uid || auth != null",
        ".read": true
      }
    },
    "users": {
      "$userId": {
        ".write": "$userId === auth.uid || auth != null",
        ".read": true
      }
    }
  }
}
